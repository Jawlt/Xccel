{
    "manifest_version": 3,
    "name": "React Chrome Extension",
    "version": "1.0",
    "description": "A Chrome extension with React integration.",
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  